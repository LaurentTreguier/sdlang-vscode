{
    "name": "sdlang",
    "displayName": "SDLang",
    "description": "Support for the Simple Declarative Language (http://sdlang.org)",
    "version": "0.1.0",
    "publisher": "LaurentTreguier",
    "license": "SEE LICENSE IN LICENSE",
    "icon": "images/sdlang.png",
    "homepage": "https://github.com/LaurentTreguier/sdlang-vscode",
    "bugs": {
        "url": "https://github.com/LaurentTreguier/sdlang-vscode/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/LaurentTreguier/sdlang-vscode.git"
    },
    "keywords": [
        "simple",
        "declarative",
        "language"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "sdl",
                "aliases": [
                    "SDL",
                    "sdl"
                ],
                "extensions": [
                    ".sdl"
                ],
                "configuration": "./sdl.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sdl",
                "scopeName": "source.sdl",
                "path": "./syntaxes/sdl.json"
            }
        ]
    }
}